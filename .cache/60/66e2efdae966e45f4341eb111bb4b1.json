rce":"SearchParams.js","original":{"line":57,"column":12}},{"generated":{"line":67,"column":196},"source":"SearchParams.js","original":{"line":57,"column":13}},{"generated":{"line":67,"column":212},"source":"SearchParams.js","original":{"line":57,"column":12}},{"generated":{"line":68,"column":0},"source":"SearchParams.js","original":{"line":57,"column":21}},{"generated":{"line":68,"column":4},"source":"SearchParams.js","original":{"line":57,"column":21}},{"generated":{"line":68,"column":8},"source":"SearchParams.js","original":{"line":57,"column":25}},{"name":"pets","generated":{"line":68,"column":10},"source":"SearchParams.js","original":{"line":57,"column":27}},{"generated":{"line":69,"column":0},"source":"SearchParams.js","original":{"line":57,"column":12}},{"generated":{"line":69,"column":4},"source":"SearchParams.js","original":{"line":38,"column":8}},{"generated":{"line":69,"column":5},"source":"SearchParams.js","original":{"line":37,"column":4}},{"generated":{"line":70,"column":0},"source":"SearchParams.js","original":{"line":61,"column":1}},{"generated":{"line":70,"column":1},"source":"SearchParams.js","original":{"line":8,"column":0}},{"name":"SearchParams","generated":{"line":72,"column":15},"source":"SearchParams.js","original":{"line":63,"column":15}}],"sources":{"SearchParams.js":"import React, { useState, useEffect } from 'react';\r\nimport pet, { ANIMALS } from \"@frontendmasters/pet\"\r\nimport useDropdown from './common/useDropdown';\r\n\r\nimport Results from './Results'\r\n\r\n\r\nconst SearchParams = () => {\r\n    const [location, setLocation] = useState(\"Seatle,Wa\");\r\n    const [breeds, setBreeds] = useState([]);\r\n\r\n    const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\r\n    const [breed, BreedDropdown, setBreed] = useDropdown(\"Breed\", \"\", breeds);\r\n\r\n    const [pets, setPets] = useState([]);\r\n\r\n    async function requestPets() {\r\n        const { animals } = await pet.animals({\r\n            location,\r\n            breed,\r\n            type: animal\r\n        });\r\n        setPets(animals || []);\r\n    }\r\n\r\n\r\n    useEffect(() => {\r\n        setBreeds([]);\r\n        setBreed();\r\n        pet.breeds(animal).then(({ breeds }) => {\r\n            const breedStrings = breeds.map(({ name }) => name);\r\n            setBreeds(breedStrings);\r\n        }, console.error)\r\n\r\n    }, [animal, setBreed, setBreeds]);\r\n\r\n    return (\r\n        <div className=\"search-params\">\r\n            <form onSubmit={e => {\r\n                e.preventDefault();\r\n                requestPets();\r\n            }} >\r\n                <label htmlFor=\"location\">\r\n                    <h1>{location}\r\n                    </h1>\r\n                    <input id=\"location\"\r\n                        value={location}\r\n                        placeholder=\"Location\"\r\n                        onChange={e => setLocation(e.target.value)} />\r\n\r\n                </label>\r\n\r\n                <AnimalDropdown />\r\n                <BreedDropdown />\r\n                <button>Submit</button>\r\n            </form>\r\n            <Results pets={pets} />\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default SearchParams;\r\n"},"lineCount":null}},"error":null,"hash":"e909276cd76c062ce03b29f8025cf10a","cacheData":{"env":{}}}